char	*my_strcat(char *str1, char *str2)
{
  int 	i;
  int	j;
  
  j = 0;
  i = 0;
  while (str1[i] != '\0')
    {
      i += 1;
    }
  while (str2[j] != '\0')
    {
      str1[i] = str2[j];
      i += 1;
      j += 1;
    }
  return (str1);
}
